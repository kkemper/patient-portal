<p id="notice"><%= notice %></p>

<h1>Listing Demographics</h1>

<table>
  <thead>
    <tr>
      <th>Demoid</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Country</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @demographics.each do |demographic| %>
      <tr>
        <td><%= demographic.demoID %></td>
        <td><%= demographic.address %></td>
        <td><%= demographic.city %></td>
        <td><%= demographic.state %></td>
        <td><%= demographic.zipcode %></td>
        <td><%= demographic.country %></td>
        <td><%= demographic.longitude %></td>
        <td><%= demographic.latitude %></td>
        <td><%= link_to 'Show', demographic %></td>
        <td><%= link_to 'Edit', edit_demographic_path(demographic) %></td>
        <td><%= link_to 'Destroy', demographic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Demographic', new_demographic_path %>
