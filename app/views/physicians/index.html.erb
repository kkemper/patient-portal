<p id="notice"><%= notice %></p>

<h1>Listing Physicians</h1>

<table>
  <thead>
    <tr>
      <th>Physicianid</th>
      <th>Demoid</th>
      <th>Fname</th>
      <th>Lname</th>
      <th>Specialty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @physicians.each do |physician| %>
      <tr>
        <td><%= physician.physicianID %></td>
        <td><%= physician.demoID %></td>
        <td><%= physician.fName %></td>
        <td><%= physician.lname %></td>
        <td><%= physician.specialty %></td>
        <td><%= link_to 'Show', physician %></td>
        <td><%= link_to 'Edit', edit_physician_path(physician) %></td>
        <td><%= link_to 'Destroy', physician, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Physician', new_physician_path %>
